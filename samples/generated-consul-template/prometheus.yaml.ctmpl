{{- $application_name := "prometheus" -}}
global:
  scrape_interval: 1m
  scrape_timeout: 1m
  evaluation_interval: 30s
  external_labels:
    application: prometheus
{{ with $d := key (printf "infrastructure/%s/config.json" $application_name) | parseJSON }}{{ $d.config.prometheus_config | toYAML }}{{ end }}